{
	"name": "imponeer/object-errors",
	"description": "Library that adds a possibility to collect errors for objects",
	"keywords": [
		"errors",
		"collection",
		"object"
	],
	"type": "library",
	"require-dev": {
		"phpunit/phpunit": "^12.0",
		"squizlabs/php_codesniffer": "^3.9 || ^4.0",
		"phpstan/phpstan": "^2"
	},
	"require": {
		"ext-json": "*",
		"php": ">=8.3"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Raimondas Rimkeviƒçius",
			"email": "mekdrop@impresscms.org"
		}
	],
	"autoload": {
		"psr-4": {
			"Imponeer\\ObjectErrors\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Imponeer\\ObjectErrors\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "phpunit",
		"phpcs": "phpcs",
		"phpcbf": "phpcbf",
		"phpstan": "phpstan analyse --memory-limit=1G"
	}
}
